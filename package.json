{
  "name": "dont-play-with-gp-web",
  "version": "0.1.4",
  "scripts": {
    "ng": "ng",
    "start": "ng serve --base-href /en/",
    "start-en": "ng serve --base-href /en/ --configuration=en",
    "start-ru": "ng serve --base-href /ru/ --configuration=ru",
    "start-fr": "ng serve --base-href /fr/ --configuration=fr",
    "build": "ng build",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "build-i18n-prod:fr": "ng build --output-path=dist/fr --aot --prod --base-href /fr/ --i18n-file=src/locale/messages.fr.xlf --i18n-format=xlf --i18n-locale=fr",
    "build-i18n-prod:ru": "ng build --output-path=dist/ru --aot --prod --base-href /ru/ --i18n-file=src/locale/messages.ru.xlf --i18n-format=xlf --i18n-locale=ru",
    "build-i18n-prod:en": "ng build --output-path=dist/en --aot --prod --base-href /en/ --i18n-file=src/locale/messages.en.xlf --i18n-format=xlf --i18n-locale=en",
    "build-i18n-prod": "npm run build-i18n-prod:en && npm run build-i18n-prod:ru && npm run build-i18n-prod:fr",
    "build-i18n-stage:fr": "ng build --output-path=dist/dont-play-with-gp-web/fr --aot --configuration=stage-fr --base-href /dont-play-with-google/fr/ --deploy-url /dont-play-with-google/fr/ --i18n-file=src/locale/messages.fr.xlf --i18n-format=xlf --i18n-locale=fr",
    "build-i18n-stage:ru": "ng build --output-path=dist/dont-play-with-gp-web/ru --aot --configuration=stage-ru --base-href /dont-play-with-google/ru/ --deploy-url /dont-play-with-google/ru/ --i18n-file=src/locale/messages.ru.xlf --i18n-format=xlf --i18n-locale=ru",
    "build-i18n-stage:en": "ng build --output-path=dist/dont-play-with-gp-web/en --aot --configuration=stage-en --base-href /dont-play-with-google/en/ --deploy-url /dont-play-with-google/en/ --i18n-file=src/locale/messages.en.xlf --i18n-format=xlf --i18n-locale=en",
    "build-i18n-stage": "npm run build-i18n-stage:en && npm run build-i18n-stage:ru && npm run build-i18n-stage:fr"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^10.0.9",
    "@angular/cdk": "^10.1.3",
    "@angular/common": "~10.0.9",
    "@angular/compiler": "~10.0.9",
    "@angular/core": "~10.0.9",
    "@angular/flex-layout": "8.0.0-beta.27",
    "@angular/forms": "~10.0.9",
    "@angular/localize": "~10.0.9",
    "@angular/material": "^10.1.3",
    "@angular/platform-browser": "~10.0.9",
    "@angular/platform-browser-dynamic": "~10.0.9",
    "@angular/router": "~10.0.9",
    "@ngx-translate/i18n-polyfill": "^1.0.0",
    "angular-markdown-editor": "^2.0.2",
    "jquery": "^3.4.1",
    "ngx-infinite-scroll": "^8.0.0",
    "ngx-markdown": "^10.1.0",
    "ngx-material-file-input": "^2.1.1",
    "rxjs": "~6.6.2",
    "tslib": "^2.0.0",
    "zone.js": "~0.10.2"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.1000.6",
    "@angular/cli": "~10.0.6",
    "@angular/compiler-cli": "~10.0.9",
    "@angular/language-service": "~10.0.9",
    "@types/jasmine": "~3.4.0",
    "@types/jasminewd2": "~2.0.3",
    "@types/jquery": "^3.3.31",
    "@types/node": "^12.11.1",
    "codelyzer": "^6.0.0",
    "jasmine-core": "~3.5.0",
    "jasmine-spec-reporter": "~5.0.0",
    "karma": "~5.0.0",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage-istanbul-reporter": "~3.0.2",
    "karma-jasmine": "~3.3.0",
    "karma-jasmine-html-reporter": "^1.5.0",
    "protractor": "~7.0.0",
    "ts-node": "~8.4.1",
    "tslint": "~6.1.0",
    "typescript": "^3.8.3"
  }
}
